
@{
    ViewBag.Title = "ToDoList";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Mvc5 ile Web Geliştirme</title>
    <!-- for-mobile-apps -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Profile Widget Responsive, Login form web template, Sign up Web Templates, Flat Web Templates, Login signup Responsive web template, Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <!-- //for-mobile-apps -->
    <link href='//fonts.googleapis.com/css?family=Poppins:400,600,500,700' rel='stylesheet' type='text/css'>
    <link href="~/web/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <script src="~/web/js/jquery-1.11.1.min.js"></script>
    <!-- //js -->
    <!-- Using Jquery -->
    <script>
        jQuery(document).ready(function ($) {
            $('#sidebar-btn').click(function () {
                $('#sidebar').toggleClass('visible');
            });
        });
    </script>
    <!--circlechart-->
    <script src="~/web/js/jquery.circlechart.js"></script>
    <!--circlechart-->

</head>
<body>
    <div class="content">
        <h1>Yapılacaklar Listesi</h1>
        <div class="main">
            <div class="header">
                <div class="hd-img">
                    <img src="~/web/images/man1.jpg" alt=" " class="img-responsive" />
                </div>

                <h2>Hasan Çiçek</h2>
                <p class="photo">Yazılım Uzmanı</p>
                <div class="skill-info">
                    <ul>
                        <li><div class="demo demo-1" data-percent="56"></div><span>Yapılacaklar</span></li>
                        <li><div class="demo demo-2" data-percent="80"></div><span>Tamamlananlar</span> </li>
                        <li><div class="demo demo-3" data-percent="45"></div><span>Devam Edenler</span> </li>
                    </ul>
                    <script>
                        $('.demo-1').percentcircle({
                            animate: true,
                            fillColor: 'rgb(119, 161, 173)'
                        });
                        $('.demo-2').percentcircle({
                            animate: true,
                            fillColor: 'rgb(0, 197, 255)'
                        });
                        $('.demo-3').percentcircle({
                            animate: true,
                            fillColor: 'rgb(119, 221, 255)'
                        });
                    </script>
                </div>
            </div>
            <section class="slider">
                <div class="flexslider">
                    <ul class="slides">
                        <li>
                            <div class="header-bottom_3">
                                <div class="jan">
                                    <h3>Temmuz 2024</h3>
                                </div>
                                <div class="jan-bottom">
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>IK Toplantısı</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>08:45</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Öğle Yemeği</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>12:00</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Tasarım Toplantısı</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>16:00</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Akşam Yemeği</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>20:00</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="header-bottom_3">
                                <div class="jan">
                                    <h3>Haziran 2024</h3>
                                </div>
                                <div class="jan-bottom">
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Spor</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>07:00</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>

                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Öğle Yemeği</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>13:00</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Planlama Toplantısı</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>15:45</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                    <div class="jan-bottom-grid">

                                        <div class="jan-bottom-grid-left1">
                                            <p>Akşam Yemeği</p>
                                        </div>
                                        <div class="jan-bottom-grid-right">
                                            <p>21:00</p>
                                        </div>
                                        <div class="clear"></div>
                                    </div>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </section>
            <!--FlexSlider-->
            <link rel="stylesheet" href="~/web/css/flexslider.css" type="text/css" media="screen" />
            <script defer src="~/web/js/jquery.flexslider.js"></script>
            <script type="text/javascript">
                $(window).load(function () {
                    $('.flexslider').flexslider({
                        animation: "slide",
                        start: function (slider) {
                            $('body').removeClass('loading');
                        }
                    });
                });
            </script>
            <!--End-slider-script-->
        </div>
        <p class="footer">&copy; @DateTime.Now.Year Tüm Hakları Saklıdır. Design by <a href="http://w3layouts.com/"> W3layouts</a></p>
        <br />
        <div class="container">
            <a href="/Test/UserList" class="btn btn-danger" style="text-align:center">Ana Sayfaya Dön</a>
        </div>
    </div>
</body>
</html>


